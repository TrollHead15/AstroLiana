# Telegram Bot Configuration
# Used for sending notifications and messages via Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Telegram Chat ID
# The ID of the Telegram chat where messages will be sent
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Resend Email Service API Key
# Used for sending emails through the Resend service
RESEND_API_KEY=your_resend_api_key_here

# Mapbox API Key (Public)
# Used for map functionality on the frontend
NEXT_PUBLIC_MAPBOX_API_KEY=your_mapbox_api_key_here

# PostHog Analytics Key (Public)
# Used for product analytics and tracking on the frontend
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_key_here

# PostHog Host (Optional)
# Custom PostHog instance host URL
# POSTHOG_HOST=https://your-posthog-host.com

# Site URL (Public)
# The public URL of the website
NEXT_PUBLIC_SITE_URL=https://lianaastro.ru
